{
  "name": "electron-vue-cli3",
  "author": "ZJINH",
  "description": "electron vue cli3",
  "appId": "com.zjinh.app.test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service lint && vue-cli-service electron:serve",
    "build": "vue-cli-service lint && vue-cli-service electron:build",
    "dev:web": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.5",
    "electron-updater": "^4.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.1",
    "electron": "9.4.0",
    "electron-icon-builder": "^1.0.0",
    "eslint": "^6.7.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "vue-cli-plugin-electron-builder": "^1.3.1",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.5.21"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "main": "background.js"
}
