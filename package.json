{
  "name": "electron-vue-cli3",
  "author": "ZJINH",
  "description": "electron vue cli3",
  "appId": "com.zjinh.app.test",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service lint && vue-cli-service electron:serve",
    "build": "vue-cli-service lint && vue-cli-service electron:build",
    "dev:web": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "core-js": "^3.18.0",
    "electron-updater": "^4.3.9",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "electron": "^14.0.1",
    "electron-icon-builder": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.14"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "main": "background.js"
}
