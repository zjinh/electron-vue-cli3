{
  "name": "electron-vue-cli3",
  "author": "ZJINH",
  "version": "2.0.0",
  "private": true,
  "appId": "com.zjinh.app.test",
  "main": "background.js",
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service lint && vue-cli-service electron:serve",
    "build": "vue-cli-service lint && vue-cli-service electron:build",
    "dev:web": "vue-cli-service serve",
    "build:web": "vue-cli-service build",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten"
  },
  "dependencies": {
    "@types/lodash": "^4.14.149",
    "axios": "^0.21.1",
    "core-js": "^3.18.0",
    "electron-updater": "^4.3.9",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "electron": "^13.4.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "prettier": "^2.2.1",
    "sass-loader": "^8.0.2",
    "typescript": "~4.1.5",
    "vue-cli-plugin-electron-builder": "^1.3.1",
    "vue-template-compiler": "^2.6.11"
  }
}
